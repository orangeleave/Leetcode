public class Solution {
    public int[][] multiply(int[][] A, int[][] B) {
        int m = A.length, n = A[0].length, k = B[0].length;
        int[][] C = new int[m][k];
        for (int i = 0; i < m; i++) {
            for (int l = 0; l < n; l++) {
                if (A[i][l] != 0) {
                    for (int j = 0; j < k; j++) {
                        if (B[l][j] != 0)   C[i][j] += A[i][l] * B[l][j];
                    }
                }
            }
        }
        return C;
    }
}
